# FEMA Implementation Environment Configuration
NODE_ENV=production
PORT=8003
LOG_LEVEL=info

# Agency configuration
AGENCY_ID=fema
AGENCY_NAME=Federal Emergency Management Agency
AGENCY_CONFIG_PATH=/app/config/agency.json

# API configuration
FOUNDATION_API_URL=http://foundation-api:8000
FOUNDATION_API_KEY=change_this_to_a_secure_api_key_in_production

# Federation configuration
FEDERATION_GATEWAY_URL=http://federation-gateway:8010
FEDERATION_CLIENT_ID=fema
FEDERATION_CLIENT_SECRET=change_this_to_a_secure_secret_in_production

# Emergency response configuration
EMERGENCY_RESPONSE_INTERVAL=5m
DISASTER_MODEL_VERSION=3.0.1
ENABLE_REAL_TIME_ALERTS=true

# Logging configuration
LOG_FORMAT=json
LOG_FILE=/var/log/fema/api.log